<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>ContactController</servlet-name>
        <servlet-class>com.controller.ContactController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhoneBookController</servlet-name>
        <servlet-class>com.controller.PhoneBookController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ContactController</servlet-name>
        <url-pattern>/ContactController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhoneBookController</servlet-name>
        <url-pattern>/PhoneBookController</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout> 30 </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>
